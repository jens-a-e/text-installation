
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT20'
	option country 'DE'
	option txpower '20'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option encryption 'psk-mixed'
	option key 't3xtinstallation'
	option ssid 'installation_admin'
	option network 'lan'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option encryption 'none'
	option network 'spot'
	option ssid 'text'

